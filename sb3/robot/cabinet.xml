<!--储物柜环境-->
<mujoco model="cabinet">
    <asset>
        <material name="MatMetal" specular="1" shininess="0.3" rgba="0.9 0.9 0.9 1"/>
        <material name="MatDarkWood" texrepeat="3 3" specular="0.4" shininess="0.1"/>
        <material name="MatLightWood" texrepeat="3 3" specular="0.4" shininess="0.1"/>
    </asset>
    <body name="cabinet" pos="-0.5 0 0">
        <!--按压任务-->
        <body name="top board" pos="0 0 0.565">
            <geom size=".2 .2 .01" pos="0 0 0" type="box" solimp="0.1 0.3 0.001 0.5 2" solref="-50000.0 -0.1"
                  rgba="0 0 0 1" group="1" condim="4"/>
            <inertial pos="0 0 0" mass="1000" diaginertia="0.1 0.1 0.1"/>
        </body>
        <!--抽屉任务-->
        <body name="drawer" pos="0.01 0 0.42">
            <!--底部-->
            <geom size=".175 .1625 .0025" pos="0 0 -0.0025" type="box" rgba="1 1 1 1" group="1" condim="4"/>
            <!--侧面-->
            <geom size=".175 .0075 .035" pos="0 -0.1550 0.035" type="box" rgba="1 1 1 1" group="1" condim="4"/>
            <geom size=".175 .0075 .035" pos="0 0.1550 0.035" type="box" rgba="1 1 1 1" group="1" condim="4"/>
            <!--后面-->
            <geom size=".0075 .1475 .035" pos="-0.1675 0 0.035" type="box" rgba="1 1 1 1" group="1" condim="4"/>
            <!--前面-->
            <geom size=".0075 .19 .075" pos="0.1825 0 0.055" type="box" rgba="1 1 1 1" group="1" condim="4"/>
            <inertial pos="0 0 0" mass="1" diaginertia="0.1 0.1 0.1"/>
            <joint name="drawer joint" type="slide" pos="0 0 0" axis="1 0 0" limited="true" range="0 0.3"/>
        </body>
        <!--门任务-->
        <body name="door" pos="0.1925 0 0.22">
            <geom name="panel" size="0.0075 0.19 .17" type="box" friction="1 1 1" material="MatDarkWood" condim="4"/>
            <inertial pos="0.0296816 -0.00152345 0" quat="0.701072 0 0 0.713091"
                      mass="2.43455" diaginertia="0.0913751 0.0521615 0.043714"/>
            <joint name="door joint" pos="0 0.19 0" axis="0 0 1" range="0.0 1.5707" damping="1" frictionloss="1"
                   limited="true"/>
        </body>
        <!--无关接触-->
        <body name="bottem board" pos="0 0 0.025">
            <geom size=".18 .19 .025" pos="0 0 0" type="box" rgba="0 0 0 1" group="1" condim="4"/>
            <inertial pos="0 0 0" mass="1000" diaginertia="0.1 0.1 0.1"/>
        </body>
        <body name="left board" pos="-0.01 0.1925 0.3025">
            <geom size=".19 .0075 0.2525" pos="0 0 0" type="box" rgba="1 1 1 0.5" group="1" condim="4"/>
            <inertial pos="0 0 0" mass="1" diaginertia="0.1 0.1 0.1"/>
        </body>
        <body name="right board" pos="-0.01 -0.1925 0.3025">
            <geom size=".19 .0075 0.2525" pos="0 0 0" type="box" rgba="0.5 0.5 0.5 0.5" group="1" condim="4"/>
            <inertial pos="0 0 0" mass="1" diaginertia="0.1 0.1 0.1"/>
        </body>
        <body name="back board" pos="-0.1925 0 0.3025">
            <geom size=".0075 .185 0.2525" pos="0 0 0" type="box" rgba="1 1 1 0.5" group="1" condim="4"/>
            <inertial pos="0 0 0" mass="1" diaginertia="0.1 0.1 0.1"/>
        </body>
    </body>
</mujoco>
